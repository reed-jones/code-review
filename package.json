{
    "private": true,
    "browserslist": [
        "last 2 versions"
    ],
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development webpack --config=webpack.config.dev.js --progress",
        "mix-development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development webpack-dev-server --config=webpack.config.dev.js",
        "mix-hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/preset-env": "^7.4.5",
        "autoprefixer": "^9.5.1",
        "cross-env": "^5.1",
        "css-loader": "^2.1.1",
        "cssnano": "^4.1.10",
        "dotenv-expand": "^5.1.0",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "laravel-mix": "^4.0.7",
        "node-notifier": "^8.0.1",
        "postcss-normalize": "^7.0.1",
        "postcss-preset-env": "^6.6.0",
        "pug": "^2.0.3",
        "pug-plain-loader": "^1.0.0",
        "resolve-url-loader": "2.3.1",
        "rimraf": "^2.6.3",
        "sass": "^1.20.1",
        "sass-loader": "7.*",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "vue-loader": "^15.7.0",
        "vue-template-compiler": "^2.6.10",
        "webpack": "^4.32.2",
        "webpack-cli": "^3.3.2",
        "webpack-dev-server": "^3.5.1",
        "webpack-manifest-plugin": "^2.0.4",
        "webpack-shell-plugin": "^0.5.0"
    },
    "dependencies": {
        "Vuem": "../packages/Vuem",
        "VuexMerge": "../packages/VuexMerge",
        "apollo-cache-inmemory": "^1.6.0",
        "apollo-client": "^2.6.0",
        "apollo-link": "^1.2.11",
        "apollo-link-http": "^1.5.14",
        "axios": "^0.18",
        "graphql": "^14.3.1",
        "graphql-tag": "^2.10.1",
        "vue": "^2.5.17",
        "vue-apollo": "^3.0.0-beta.30",
        "vue-router": "^3.0.6",
        "vuex": "^3.1.1"
    }
}
